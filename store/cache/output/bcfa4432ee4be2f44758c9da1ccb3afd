								<p class="comment-content">
									&#39; if 的三元形式
Function IIf(flag,c1,c2)
    If flag Then
        IIf = c1
    Else
        IIf = c2
    End If
End Function

&#39; 检查值是不是空
Function IsNullOrEmpty(value)
    If IsNull(value) Or IsEmpty(value) Or value="" Then
        IsNullOrEmpty = True
    Else
        IsNullOrEmpty = False
    End If
End Function

&#39; 返回数字,类似PHP的intval()
Function Intval(value)
    Dim Matchs,m
    If IsNullOrEmpty(value) Then
        Intval = 0
    Else
        Set Matchs = RegMatch("d&#42;", value, True, False)
        Intval = Matchs(0).Value
        If IsNullOrEmpty(Intval) Then
            Intval = 0
        Else
            Intval = CInt(Intval)
        End If
    End If
End Function

&#39; 取ip
Function GetIp()
    Dim GetClientIP
    GetClientIP = Request.ServerVariables("HTTP_X_FORWARDED_FOR")
    If GetClientIP = "" or isnull(GetClientIP) or isempty(GetClientIP) Then
        GetClientIP = Request.ServerVariables("REMOTE_ADDR")
    end if
    GetIp = GetClientIP
End function								</p>
