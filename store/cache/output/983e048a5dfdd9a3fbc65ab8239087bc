					<p class="comment-content">
						case &quot;user&quot;:
		dim wner : wner = str_replace(&quot;^user-(.+?)-(.+?).html$&quot;,query,&quot;$1&quot;)
		if trim(clid)=&quot;&quot; or not isnumeric(clid) then clid = 0
		
		dim wnerpage : wnerpage = str_replace(&quot;^user-(.+?)-(.+?).html$&quot;,query,&quot;$2&quot;)
		.template = &quot;article-list&quot;
		.cachefle = &quot;article-&quot;&wner&&quot;-&quot;&wnerpage
		if .check() then
			import &quot;lib.Article&quot;
			
			set catalog = new catalog_class
			catalog.limit = &quot;cl_hide = false an&#100;  cl_upid = 0&quot;
			set navs = catalog.list()
			
			set widget = new widget_class
			widget.limit = &quot;wg_side = true&quot;
			set itmA = widget.list()
			widget.limit = &quot;wg_foot = true&quot;
			set itmB = widget.list()
			
			set article = new article_class
			article.size = 10
			article.page = wnerpage
			article.link = &quot;?user-&quot;&wner&&quot;-{$p}.html&quot;
			article.limit = &quot;ar_draft = false an&#100;  ar_owner ='&quot;&wner&&quot;'&quot;
			set post = article.list()
			turner = article.turner
			
			set user = new user_class
			
			if not post.eof then
				page_title = post(&quot;ar_clog&quot;)&&quot; - &quot;&page_title
				page_keywords = post(&quot;ar_clog&quot;)&&quot;,&quot;&page_keywords
				page_de&#115;cription = post(&quot;ar_clog&quot;)&&quot;。&quot;&page_de&#115;cription
			end if
			
			.show()
		end If					</p>
